<h1>Movies</h1>
<h2>Click on the movie to check the showtimes</h2>
<div id="movie-container">
    {{#each movies}}
    <div id="movie-div">
        <div>
            <img src="{{this.image}}" alt="">
        </div>
        <div>
            {{this.title}}
        </div>
        <div>
            <a href="/{{this.id}}">See More</a>
        </div>
    </div>
        
        

    {{/each}}
</div>
